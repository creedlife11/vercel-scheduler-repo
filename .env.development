# Development Environment Configuration
# This file contains default values for development

# Environment
VERCEL_ENV=development
NODE_ENV=development

# Authentication (disabled in development)
ENABLE_AUTH=false
# NEXTAUTH_SECRET=your-secret-here
# NEXTAUTH_URL=http://localhost:3000

# Database (optional in development)
# DATABASE_URL=file:./dev.db

# Feature Flags
MAX_WEEKS_ALLOWED=104
FAIRNESS_THRESHOLD=0.5
MAX_REQUEST_SIZE_MB=5.0

# Rate Limiting (disabled in development)
ENABLE_RATE_LIMITING=false

# Monitoring
LOG_LEVEL=DEBUG
ENABLE_METRICS=true

# Security
# CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000

# Edge Config (optional)
# EDGE_CONFIG=your-edge-config-id
# EDGE_CONFIG_TOKEN=your-edge-config-token

# Development-specific features
ENABLE_EXPERIMENTAL_FEATURES=true